{
  "name": "fin-catch-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:tauri": "turbo run dev --filter=@fin-catch/tauri",
    "dev:web": "turbo run dev --filter=@fin-catch/web",
    "build": "turbo run build",
    "build:tauri": "turbo run build --filter=@fin-catch/tauri",
    "build:web": "turbo run build --filter=@fin-catch/web",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^25.0.1",
    "prettier": "^3.2.5",
    "turbo": "^2.5.4",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.1.0"
}
